<h2>Sign uppp</h2>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "technicians/shared/error_messages", resource: resource %>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>
  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>
  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autofocus: true, autocomplete: "email" %>
  </div>
  <div class="field">
    <%= f.label :age  %><br />
    <%= f.text_field :age, autofocus: true, autocomplete: "email" %>
  </div>
  <div class="field">
    <%= f.label :nationality  %><br />
    <%= f.text_field :nationality, autofocus: true, autocomplete: "email" %>
  </div>
  <div class="field">
    <%= f.label :work_allowed  %><br />
    <%= f.text_field :work_allowed, autofocus: true, autocomplete: "email" %>
  </div>
  <div class="field">
    <%= f.label :id_ausweis  %><br />
    <%= f.text_field :id_ausweis, autofocus: true, autocomplete: "email" %>
  </div>
  <div class="field">
    <%= f.label :inumber_of_years_work_experiance  %><br />
    <%= f.text_field :number_of_years_work_experiance, autofocus: true %>
  </div>
  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>
<%= render "technicians/shared/links" %>
